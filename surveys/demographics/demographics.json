{
  "title": "Demographics",
  "logoPosition": "right",
  "pages": [
    {
      "name": "universal",
      "elements": [
        {
          "type": "text",
          "name": "birth_year",
          "title": "What year were you born?",
          "valueName": "birth_year",
          "isRequired": true,
          "validators": [
            {
              "type": "numeric",
              "minValue": 1900,
              "maxValue": 2022
            }
          ],
          "placeholder": "4-digit year (e.g. 1839)"
        },
        {
          "type": "radiogroup",
          "name": "gender",
          "title": "What is your gender?",
          "choices": [
            {
              "value": "female",
              "text": "Female"
            },
            {
              "value": "male",
              "text": "Male"
            },
            {
              "value": "other",
              "text": "Other"
            }
          ]
        },
        {
          "type": "text",
          "name": "gender_other",
          "visibleIf": "{gender} = 'other'",
          "title": "Please enter your gender below:"
        },
        {
          "type": "dropdown",
          "name": "marital_status",
          "title": "What is your marital status?",
          "choices": [
            "Single Never Married",
            "Married or Domestic Partnership",
            "Widowed",
            "Divorced",
            "Separated",
            "Other"
          ],
          "autocomplete": "country"
        },
        {
          "type": "checkbox",
          "name": "impairment",
          "title": "Do you have any health conditions or illnesses which affect you in any of the following areas?",
          "description": "In answering this question, you should consider whether you are affected in any of these areas while receiving any treatment or medication or using devices to help you. (For example, a hearing aid.)",
          "choices": [
            {
              "value": "vision",
              "text": "Vision (e.g. blindness or partial sight)"
            },
            {
              "value": "hearing",
              "text": "Hearing (e.g. deafness or partial hearing)"
            },
            {
              "value": "mobility",
              "text": "Mobility (e.g. walking short distances or climbing stairs)"
            },
            {
              "value": "dexterity",
              "text": "Dexterity (e.g. lifting and carrying objects, using a keyboard)"
            },
            {
              "value": "understanding",
              "text": "Learning or understanding or concentrating"
            },
            {
              "value": "memory",
              "text": "Memory"
            },
            {
              "value": "mental_health",
              "text": "Mental health"
            },
            {
              "value": "stamina",
              "text": "Stamina or breathing or fatigue"
            },
            {
              "value": "sociobehavioral",
              "text": "Social or behavioral (e.g. autism spectrum disorder (ASD) which includes Asperger’s, or attention deficit hyperactivity disorder (ADHD))"
            },
            {
              "value": "none",
              "text": "None of the above / Prefer not to say"
            }
          ],
          "separateSpecialChoices": true,
          "noneText": "None of the above",
          "maxSelectedChoices": 12
        },
        {
          "type": "dropdown",
          "name": "language_primary",
          "title": "What is your primary language?",
          "description": "This is the language you use most naturally, for example, the language you use at home.",
          "isRequired": true,
          "choices": [
            "Arabic",
            "Armenian",
            "Cambodian",
            "Chinese",
            "Czech",
            "Dutch",
            "English",
            "French",
            "French Creole",
            "German",
            "Greek",
            "Gujarathi",
            "Hebrew",
            "Hindi",
            "Hungarian",
            "Italian",
            "Japanese",
            "Korean",
            "Navaho",
            "Persian",
            "Polish",
            "Portuguese",
            "Russian",
            "Spanish",
            "Tagalog",
            "Thai",
            "Ukrainian",
            "Vietnamese",
            "Yiddish",
